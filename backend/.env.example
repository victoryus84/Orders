# Sample environment variables for development
# Copy this file to .env and fill values before running the backend locally.

# Postgres DSN (used in config.Load). Example format:
# host=localhost user=postgres password=postgres dbname=orders_dev port=5432 sslmode=disable TimeZone=UTC
DSN=host=localhost user=postgres password=postgres dbname=orders_dev port=5432 sslmode=disable TimeZone=UTC

# JWT secret for signing tokens (use a long random string in real dev)
JWT_SECRET=change_me_dev_secret

# Server port
PORT=8080
